<html>
    <head>
        <title>Dashboard</title>
        <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
    </head>
    <body>
        <div id="chart" style="max-width: 100%; height: 400px;"></div>
        <script>
            var chart = new ApexCharts(document.querySelector("#chart"), {{options}});

            chart.render();
        </script>
    </body>
</html>
